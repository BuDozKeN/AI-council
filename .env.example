# AI Council Environment Variables
# Copy this file to .env and fill in your actual values

# OpenRouter API (Required)
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Supabase (Required)
# Get these from your Supabase project settings
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Service Key (Optional - for admin operations)
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Stripe (Optional - only needed for billing features)
# Get these from: https://dashboard.stripe.com/apikeys
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret

# Mock Mode (Optional - for testing without API calls)
# Set to "true" to enable mock LLM responses
MOCK_LLM=false
MOCK_LLM_SCENARIO=default
